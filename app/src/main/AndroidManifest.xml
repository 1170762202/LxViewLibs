<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zlx.lxviewlibs">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.drag_view.PhotoBrowseAc"
            android:theme="@style/FullScreenStyle" />
        <activity android:name=".ui.drag_view.DragViewAc" />
        <activity android:name=".ui.combine_view.CombineAc" />
        <activity android:name=".ui.coordinator.CoordinatorAc" />
        <activity android:name=".ui.circle_image_view.CircleImgAc" />
    </application>

</manifest>